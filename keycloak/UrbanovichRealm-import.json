{
  "realm": "UrbanovichRealm",
  "enabled": true,
  "loginWithEmailAllowed": true,
  "registrationAllowed": true,
  "resetPasswordAllowed": true,
  "duplicateEmailsAllowed": false,
  "roles": {
    "realm": [
      { "name": "POWER-USER" }
    ]
  },
  "clients": [
    {
      "clientId": "urbanovich-ui",
      "name": "urbanovich-ui",
      "enabled": true,
      "protocol": "openid-connect",
      "publicClient": false,
      "secret": "urbanovich-ui-secret",
      "standardFlowEnabled": true,
      "directAccessGrantsEnabled": true,
      "serviceAccountsEnabled": false,
      "redirectUris": [
        "https://localhost:7001/*",
        "http://localhost:7001/*",
        "http://localhost:5125/*"
      ],
      "webOrigins": ["*"],
      "attributes": {
        "post.logout.redirect.uris": "+"
      }
    },
    {
      "clientId": "urbanovich-service",
      "name": "urbanovich-service",
      "enabled": true,
      "protocol": "openid-connect",
      "publicClient": false,
      "secret": "urbanovich-service-secret",
      "standardFlowEnabled": false,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": true
    }
  ],
  "users": [
    {
      "username": "service-account-urbanovich-service",
      "enabled": true,
      "serviceAccountClientId": "urbanovich-service",
      "realmRoles": ["POWER-USER"],
      "clientRoles": {
        "realm-management": ["manage-users", "view-users", "query-users"]
      }
    }
  ]
}
